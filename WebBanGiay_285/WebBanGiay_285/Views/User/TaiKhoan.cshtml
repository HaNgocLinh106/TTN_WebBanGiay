@model List<Model.EF.GioHang>


@{
    ViewBag.Title = "Tài khoản";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<br />
<br />
<h2 class="text-center">Tài khoản của bạn</h2>
<br />
<br />

<div class="container">

    <table id="cart" class="table table-hover table-condensed">

        <tbody>
            @foreach (var it in Model)
            {
                <tr>

                    <td data-th="Product">
                        <div class="row">

                            <div class="col-sm-10">
                                <h4 class="nomargin">@it.TenNguoiDung</h4>

                            </div>
                        </div>
                    </td>
                    <td data-th="Price">@it.SDT</td>

                </tr>

                <tr class="visible-xs">
                </tr>
                <tr>

                    <td colspan="2" class="hidden-xs"> </td>

                    <td>
                        <a href="/Home/Index" id="btnPayment" class="btn btn-success btn-block">Trang chủ Sport Shop <i class="fa fa-angle-right"></i></a>
                    </td>
                    <td>
                        <a href="tai-khoan-khach-hang/@it.SDT-@it.MaGioHang" class="btn btn-success btn-block">Tài khoản của bạn <i class="fa fa-angle-right"></i></a>
                    </td>
                </tr>

            }
    </table>
</div>




